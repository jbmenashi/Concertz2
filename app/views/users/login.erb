
<h1>TicketFaster</h1>

<div class="login_form">
  <%= form_tag "/login"  do %>
    <%= label_tag "Username:" %>
    <%= text_field_tag :username %>
    <%= submit_tag "Login!", class: "ui purple button"%>
  <% end %>
</div>

<div class="error">
  <% if flash[:error]  %>
    <%= flash[:error] %>
  <% end %>
</div>
